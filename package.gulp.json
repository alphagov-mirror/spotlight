{
  "name": "spotlight",
  "version": "0.0.1",
  "description": "Hybrid rendering app for the GOV.UK Performance Platform using Backbone and D3",
  "bugs": "https://github.com/alphagov/spotlight/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alphagov/spotlight.git"
  },
  "engines": {
    "node": "0.10.26"
  },
  "private": true,
  "scripts": {
    "build:production": "./node_modules/.bin/gulp build:production",
    "supervisor": "./node_modules/supervisor/lib/cli-wrapper.js -w app -i app/vendor -e 'js|html' -n error app/server",
    "lint": "[ -z \"$LINTFILES\" ] && LINTFILES=\"** app/support/**/**/*.json\"; ./node_modules/jshint/bin/jshint ${LINTFILES}",
    "test": "grunt test:all",
    "shell:cheapseats": "node ./node_modules/cheapseats/index.js --reporter dot-retry --standalone --path --quickrun ./",
    "shell:cheapseats_full_run": "node ./node_modules/cheapseats/index.js --reporter dot-retry --standalone --path ./",
    "test:functional": "node ./tests/tools/server_and_test.js -e phantomjs",
    "test:functional:ci": "node ./tests/tools/server_and_test.js",
    "test:functional:ff": "./node_modules/nightwatch/bin/nightwatch",
    "test:functional:all": "./node_modules/nightwatch/bin/nightwatch -e default,chrome,phantomjs",
    "jasmine_node": "grunt jasmine_node",
    "jasmine": "grunt jasmine",
    "test:unit": "grunt test:unit",
    "start": "gulp",
    "jasmine:spotlight:build": "grunt jasmine:spotlight:build",
    "tests": "./node_modules/.bin/gulp tests",
    "tests:jasmine": "./node_modules/.bin/gulp tests:jasmine",
    "tests:jasmine_node": "./node_modules/.bin/gulp tests:jasmine_node",
    "watch": "./node_modules/.bin/gulp watch"
  },
  "dependencies": {
    "argh": "0.1.4",
    "backbone": "1.1.0",
    "chromedriver": "2.29.0",
    "co": "3.0.6",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "d3": "3.3.7",
    "errorhandler": "1.5.0",
    "express": "4.15.2",
    "glob": "4.0.2",
    "govuk_frontend_toolkit": "4.9.1",
    "govuk_template_mustache": "0.12.0",
    "graceful-fs": "4.1.11",
    "del": "2.2.2",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.1",
    "gulp-flatten": "0.3.1",
    "gulp-rename": "1.2.2",
    "gulp-requirejs": "0.4.0",
    "gulp-sass": "3.1.0",
    "gulp-nodemon": "2.2.1",
    "gulp-sourcemaps": "1.11.1",
    "gulp-watch": "4.3.11",
    "gulp-batch": "1.0.5",
    "gulp-rev": "7.1.2",
    "gulp-jasmine": "2.4.2",
    "gulp-jasmine-node": "1.0.7",
    "gulp-jshint": "2.0.4",
    "run-sequence": "1.2.2",
    "jquery": "1.8.3",
    "jquery-deparam": "0.1.0",
    "jshint": "2.9.4",
    "jshint-stylish": "2.2.1",
    "jsonschema": "1.1.1",
    "lodash": "2.4.1",
    "moment-timezone": "0.2.4",
    "morgan": "1.1.1",
    "mustache": "2.3.0",
    "nightwatch": "0.9.14",
    "node-basicauth": "0.0.9",
    "node-sass": "4.5.2",
    "node-statsd": "0.0.7",
    "nopt": "4.0.1",
    "optimist": "0.6.1",
    "phantomjs": "2.1.7",
    "q": "1.0.1",
    "requirejs": "2.1.14",
    "sanitizer": "0.1.1",
    "selenium-server": "2.43.1",
    "serialize-javascript": "^1.3.0",
    "spdy": "3.4.4",
    "uglify-js": "2.8.22",
    "winston": "0.8.1",
    "xmlhttprequest": "1.6.0"
  },
  "devDependencies": {
    "cheapseats": "git+https://github.com/alphagov/cheapseats#9e7e51c5ae9b1d59ffa165debc1c697e102a4388",
    "performanceplatform-js-style-configs": "0.0.2"
  }
}
